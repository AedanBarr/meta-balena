# POKY_BBLAYERS_CONF_VERSION is increased each time build/conf/bblayers.conf
# changes incompatibly
POKY_BBLAYERS_CONF_VERSION = "2"

BBPATH = "${TOPDIR}"
BBFILES ?= ""

BBLAYERS ?= " \
  /build/balena-os/layers/poky/meta \
  /build/balena-os/layers/poky/meta-poky \
  /build/balena-os/layers/poky/meta-yocto-bsp \
  /build/balena-os/layers/meta-openembedded/meta-oe \
  /build/balena-os/layers/meta-openembedded/meta-filesystems \
  /build/balena-os/layers/meta-openembedded/meta-networking \
  /build/balena-os/layers/meta-openembedded/meta-python \
  /build/balena-os/layers/meta-balena/meta-balena-common \
  /build/balena-os/layers/meta-balena/meta-balena-warrior \
  /build/balena-os/layers/meta-rust \
  "
